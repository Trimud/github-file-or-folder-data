{
  "name": "git-download-file",
  "version": "1.0.0",
  "description": "Download file from Github repo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly" : "yarn test && yarn lint",
    "preversion" : "yarn lint",
    "version" : "yarn format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trimud/git-download-file.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Trimud/git-download-file/issues"
  },
  "homepage": "https://github.com/Trimud/git-download-file#readme",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "files": [
    "dist/**/*"
  ]
}
